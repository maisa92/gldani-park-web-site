@model IEnumerable<GldaniParkFront.ViewModels.ReportListViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
 
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Show", item.Action)
        </td>
    </tr>
}

</table>
